<?xml version="1.0"?>
<Container version="2">
  <Name>newsletterr</Name>
  <Repository>jma1ice/newsletterr</Repository>
  <Registry>https://hub.docker.com/r/jma1ice/newsletterr</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/profile/277325-kosaferi/</Support>
  <Project>https://github.com/jma1ice/newsletterr</Project>
  <Overview>Newsletterr is a tool for managing email newsletters. It runs from Docker and stores data in appdata. Configure environment variables and ports as needed.</Overview>
  <Category>Media</Category>
  <WebUI>http://[IP]:[PORT]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/kosaferi/newsletterr/master/newsletterr.xml</TemplateURL>
  <Icon>https://cdn-icons-png.flaticon.com/512/10290/10290270.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>0</DateInstalled>
  <DonateText>Donate</DonateText>
  <DonateLink>http://paypal.me/kosaferi</DonateLink>
  <Requires/>
  <Config Name="Web Interface" Target="6397" Default="6397" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">6397</Config>
  <Config Name="AppData" Target="/app/data" Default="/mnt/user/appdata/newsletterr" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/newsletterr</Config>
  <Config Name="Environment Variable" Target="PUBLIC_BASE_URL" Default="http://127.0.0.1:6397" Mode="rw" Description="Base URL for public access" Type="Variable" Display="advanced" Required="false" Mask="false">http://127.0.0.1:6397</Config>
</Container>
